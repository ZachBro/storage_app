<div class="text-center">
  <h1 class="headings">Employees</h1>
</div>
<div class="form-container">
  <table class="table table-striped table-dark">
    <thead>
      <tr>
        <th>ID number</th>
        <th>Name</th>
      </tr>
      <tbody>
        <% @employees.each do |e| %>
          <tr>
            <td><%= e.id_number %></td>
            <td><%= e.name %></td>
          </tr>
        <% end %>
      </tbody>
    </thead>
  </table>
</div>
